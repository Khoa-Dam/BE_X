{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "description": "Express + TypeORM + PostgreSQL backend",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migration:generate": "typeorm-ts-node-commonjs -d src/db.ts migration:generate ./migrations/auto",
    "migration:run": "typeorm-ts-node-commonjs -d src/db.ts migration:run",
    "migration:revert": "typeorm-ts-node-commonjs -d src/db.ts migration:revert",
    "check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Khoa-Dam/BE_X.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Khoa-Dam/BE_X/issues"
  },
  "homepage": "https://github.com/Khoa-Dam/BE_X#readme",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "google-auth-library": "^10.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.26",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "ts-node-dev": "^2.0.0",
    "typeorm-ts-node-commonjs": "0.3.20",
    "typescript": "^5.9.2"
  }
}